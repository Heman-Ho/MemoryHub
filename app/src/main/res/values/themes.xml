<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MemoryHub" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/light_green_custom</item>
        <item name="colorPrimaryVariant">@color/forest_green_custom</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="ScreenBackground"><item name="android:background">@color/white</item></style>

    <style name="TextColor"><item name="android:textColor">@color/black</item></style>
    <style name="LogoChange"><item name="android:src">@drawable/output_onlinepngtools</item></style>
    <style name="BannerChange"><item name="android:src">@drawable/topbanner</item></style>
    <style name="SettingsChange"><item name="android:src">@drawable/settingsicon</item></style>
    <style name="LogoutChange"><item name="android:src">@drawable/logouticon</item></style>
    <style name="PuzzGameChange"><item name="android:src">@drawable/finalpuzzicon</item></style>
    <style name="MemGameChange"><item name="android:src">@drawable/finalmatchicon</item></style>
    <style name="ProfileBorderChange"><item name="android:color">@color/black</item></style>
    <style name="textBanner"><item name="android:src">@drawable/textbannerlight</item></style>
    <style name="matchBackground"><item name ="android:color">@color/green</item></style>






</resources>